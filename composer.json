        "tedivm/jshrink": "~1.0"
    },
    "require-dev": {
        "kint-php/kint": "^3.3",
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.22"
    },
    "autoload": {
        "psr-4": {
            "fiftyone\\pipeline\\core\\": "src/"
