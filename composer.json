        "psr-4": {
            "fiftyone\\pipeline\\core\\tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze",
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --diff --verbose",
        "php-cs-fixer-ci": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --diff --verbose --dry-run --format=junit"
